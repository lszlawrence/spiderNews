# baseURL = ["http://www.trbappcon.org/2017conf/PresentationDetails.aspx?abstractid={}".format(str(i)) for i in range(1, 400, 1)]

baseURL = "http://www.trbappcon.org/2017conf/PresentationDetails.aspx?abstractid=1"

web_data = requests.get(baseURL)
soup = BeautifulSoup(web_data.text, 'lxml')
# pprint.pprint(soup)

list = soup.find_all("a", text="Presentation")
print(list)
